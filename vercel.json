{
  "builds": [
    {
      "src": "QIOS/wsgi.py", 
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "handle": "filesystem" 
    },
    {
      "src": "/(.*)",
      "dest": "QIOS/wsgi.py"
    }
  ]
}